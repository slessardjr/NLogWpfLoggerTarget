<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd">
  <targets>
    <target xsi:type="NLogWpfLoggerTarget" name="nLogWpfLoggerTarget" />
    <target xsi:type="Console" name="console"/>
  </targets>
  <rules>
    <logger minlevel="Trace" name="*" writeTo="nLogWpfLoggerTarget"/>
    <logger minlevel="Trace" name="*" writeTo="console"/>
  </rules>
</nlog>